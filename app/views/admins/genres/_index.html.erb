<div class="genre_search">
  <div class="col-lg-2">
    <table class='table'>
    <thead>
      <tr>
        <th>ジャンル検索</th>
      </tr>
    </thead>
    <tbody>
      <% genres.each do |genre| %>
      <tr>
        <td><%= link_to genre.name, admins_items_path(genre) %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
  </div>
</div>